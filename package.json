{
  "name": "fe-boilerplate",
  "version": "1.0.2",
  "description": "A collection of useful Gulp tasks to automate your front-end workflow",
  "author": "Iain van der Wiel <iain@e-sites.nl>",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-sites/gulp-frontend-tasks.git"
  },
  "license": "MIT",
  "config": {
    "paths": {
      "tasks": "/tasks",
      "source": {
        "css": "./assets/css",
        "js": "./assets/js",
        "images": "./assets/images",
        "svg": "./assets/svg",
        "mjml": "./assets/path-to-mail"
      },
      "dist": {
        "css": "./build/css",
        "js": "./build/js",
        "images": "./build/images",
        "svg": "./build/svg"
      }
    },
    "revisionFiles": false,
    "mjml": {
      "enabled": false,
      "extension": ".twig"
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "devDependencies": {
    "@e-sites/stylelint-config-esites": "0.0.2",
    "async": "^2.1.4",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "del": "^0.1.3",
    "eslint": "^3.13.1",
    "eslint-plugin-compat": "^1.0.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.3.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-mjml": "^2.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-svgstore": "^6.0.0",
    "gulp-tasker": "^0.3.0",
    "gulp-uglify": "^2.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "bourbon": "4.3.3",
    "bourbon-neat": "1.8.0",
    "normalize.css": "^6.0.0",
    "svgxuse": "^1.2.3",
    "jquery": "^3.2.0"
  }
}
