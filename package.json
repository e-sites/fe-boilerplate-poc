{
  "name": "fe-boilerplate",
  "version": "1.0.2",
  "description": "A collection of useful Gulp tasks to automate your front-end workflow",
  "author": "Iain van der Wiel <iain@e-sites.nl>",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-sites/gulp-frontend-tasks.git"
  },
  "license": "MIT",
  "config": {
    "js": {
      "entries": [
        "app.js"
      ],
      "vendor": [
        "svgxuse",
        "jquery"
      ]
    },
    "paths": {
      "tasks": "/tasks",
      "source": {
        "css": "./assets/css",
        "js": "./assets/js",
        "images": "./assets/images",
        "svg": "./assets/svg",
        "mjml": "./assets/path-to-mail"
      },
      "dist": {
        "css": "./build/css",
        "js": "./build/js",
        "images": "./build/images",
        "svg": "./build/svg"
      }
    },
    "revisionFiles": false,
    "mjml": {
      "enabled": false,
      "extension": ".twig"
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "devDependencies": {
    "@symfony/webpack-encore": "^0.16.0",
    "async": "^2.1.4",
    "del": "^0.1.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.3.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-mjml": "^2.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-svgstore": "^6.0.0",
    "gulp-tasker": "^0.3.0",
    "webpack-stream": "^4.0.0"
  },
  "dependencies": {
    "bourbon": "4.3.3",
    "bourbon-neat": "1.8.0",
    "normalize.css": "^7.0.0",
    "svgxuse": "^1.2.3",
    "jquery": "^3.2.0"
  }
}
